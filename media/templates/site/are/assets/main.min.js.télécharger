function initMap(){function t(){var t={center:n,zoom:17,scrollwheel:!1,draggable:!1,mapTypeId:google.maps.MapTypeId.ROADMAP},i=new google.maps.Map(document.getElementById("contactMap"),t),r=new google.maps.Marker({position:n});r.setMap(i)}var n=new google.maps.LatLng(36.741616015937332,3.0943116545677185);google.maps.event.addDomListener(window,"load",t)}function showMap(n,t,i,r){var u=L.map(document.getElementById("contactMap")).setView([36.741616015937332,3.0943116545677185],17);i==="ar"?u.zoomControl.setPosition("bottomright"):u.zoomControl.setPosition("bottomleft");L.tileLayer("http://{s}.google.com/vt/lyrs=m,traffic&x={x}&y={y}&z={z}",{attribution:'&copy; <a href="https://www.google.com/maps">Google Maps<\/a>',attributionControl:!1,maxZoom:21,subdomains:["mt0","mt1","mt2","mt3"]}).addTo(u);var f="leafletPopup "+i,e="<div class='"+f+"'><h6 class='color-green-dark'>ARPCE<\/h6><div class='pb-1'>"+n+".<\/div><div><a href='"+r+"' target='_blank' class='address' rel='nofollow'>"+t+"<\/a>.<\/div><\/div>",o=L.icon({iconUrl:"/img/map/marker-red-30x40.png",shadowUrl:"/img/map/marker-shadow.png",iconSize:[30,40],shadowSize:[41,41],iconAnchor:[15,40],shadowAnchor:[11,41],popupAnchor:[0,-40]});L.marker([36.741616015937332,3.0943116545677185],{icon:o}).addTo(u).bindPopup(e).openPopup()}function shareWindow(n){var t=(screen.width-570)/2,i=(screen.height-570)/2,r="menubar=no,toolbar=no,status=no,width=570,height=570,top="+i+",left="+t;window.open(n,"NewWindow",r)}function share(n){var t=encodeURIComponent(document.URL);switch(n){case"facebook":t="https://www.facebook.com/sharer.php?u="+t;break;case"twitter":t="https://twitter.com/intent/tweet?url="+t;break;case"linkedin":t="https://www.linkedin.com/shareArticle?mini=true&url="+t}shareWindow(t)}function MouseoverMenu(n){var t=$(".current-device").css("--device");t&&t.includes("desktop")&&$(n).dropdown("toggle")}if($(document).ready(function(){$(".navbar a, a[href='#topPage']").on("click",function(n){if($(".dropdown-menu .dropdown-item-see").dropdown("hide"),this.hash!==""){if($(this.hash).offset())n.preventDefault();else return;var t=this.hash;$("html, body").animate({scrollTop:$(t).offset().top-$("#navbar").height()},800,function(){})}});$(".dropdown").on("show.bs.dropdown",function(){$(this).find(".dropdown-menu").first().stop(!0,!0).slideDown()});$(".dropdown").on("hide.bs.dropdown",function(){$(this).find(".dropdown-menu").first().stop(!0,!0).slideUp()});$(".collapse-menu").on("show.bs.collapse",function(){$(this).prev().addClass("active")});$(".collapse-menu").on("hide.bs.collapse",function(){$(this).prev().removeClass("active")});$("#popover-mobile").on("show.bs.popover",function(){$("#popover-mobile").addClass("active")});$("#popover-mobile").on("hide.bs.popover",function(){$("#popover-mobile").removeClass("active")});$("#popover-desktop").on("show.bs.popover",function(){$("#popover-desktop").addClass("active")});$("#popover-desktop").on("hide.bs.popover",function(){$("#popover-desktop").removeClass("active")});$("a[href^='tel:'], a[href^='fax:'], a[href^='mailto:']").on("click",function(){this.href&&$.post("/"+$("html")[0].lang+"/contact/click?data="+this.href.replace(":","@"))});$(".address").on("click",function(){this.href&&$.post("/"+$("html")[0].lang+"/contact/click?data=gmaps-arpce")});$(".jawdati").on("click",function(){this.href&&$.post("/"+$("html")[0].lang+"/contact/click?data=gplay-jawdati")})}),window.location.hash){var hash=window.location.hash,$hash=$(hash);$("html, body").animate({scrollTop:$(hash).offset().top-$("#navbar").height()},800,function(){window.location.hash=hash})}$(window).scroll(function(){});$(".navbar-collapse .nav-item:not(.dropdown)").click(function(){$(".navbar-collapse").collapse("hide")});$(window).click(function(n){$(n.target).closest(".navbar-collapse").length||$(".navbar-collapse").collapse("hide")});$(function(){$('[data-toggle="popover"]').popover();$("body").on("click",function(n){$('[data-toggle="popover"]').each(function(){$(this).is(n.target)||$(this).has(n.target).length!==0||$(".popover").has(n.target).length!==0||$(this).popover("hide")})})});$(function(){$('[data-toggle="tooltip"]').tooltip()});